<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity BillingAccount.
    -->
    <changeSet id="20210327100457-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="customer_subscription_id"
                                 baseTableName="billing_account"
                                 constraintName="fk_billing_account_customer_subscription_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer_subscription"/>

        <addForeignKeyConstraint baseColumnNames="invoice_address_id"
                                 baseTableName="billing_account"
                                 constraintName="fk_billing_account_invoice_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

    </changeSet>
</databaseChangeLog>
